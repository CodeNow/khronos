{
  "name": "khronos",
  "version": "1.3.1",
  "engine": "0.12.0",
  "engineStrict": true,
  "description": "scripts run via cronjobs to maintain docks",
  "main": "index.js",
  "scripts": {
    "test-watch": "node_modules/.bin/rerun-script test",
    "start": "NODE_PATH=./lib node --harmony --use_strict ./index.js",
    "test": "NODE_PATH=./lib lab -v",
    "test-dry": "NODE_PATH=./lib lab -d -v",
    "test-cov-html": "NODE_PATH=./lib lab -r html -o coverage.html",
    "dry": "NODE_ENV=staging NODE_PATH=./lib MANUAL_RUN=true DEBUG=khronos* DRY_RUN=true node --harmony --use_strict ./index.js",
    "lint": "jshint ."
  },
  "watches": {
    "test": [
      "*.js",
      "lib/**/*.js",
      "test/**/*.js",
      "scripts/**/*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/CodeNow/docker-cron.git"
  },
  "author": "Casey Flynn <casey@runnable.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CodeNow/docker-cron/issues"
  },
  "homepage": "https://github.com/CodeNow/docker-cron",
  "dependencies": {
    "101": "^1.0.0",
    "async": "^0.9.0",
    "callback-count": "^0.1.0",
    "cron": "^1.0.6",
    "debug": "^2.1.1",
    "dockerode": "^2.0.4",
    "keypather": "^1.10.1",
    "loadenv": "^1.0.3",
    "mongodb": "^1.4.29",
    "node-dogstatsd": "0.0.6",
    "request": "^2.51.0"
  },
  "devDependencies": {
    "chai": "^1.10.0",
    "colors": "^1.0.3",
    "docker-mock": "^0.6.0",
    "lab": "^5.0.2",
    "nock": "^0.59.1",
    "rerun-script": "^0.6.0",
    "rewire": "^2.3.3",
    "sinon": "^1.12.1"
  }
}
